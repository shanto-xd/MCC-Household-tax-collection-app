    <div class="container">
        <div class="row mx-auto">
            <div class="col-md-12">
               <% if (lastPage > 0) { %>
        <nav>
            <ul class="pagination offset-md-3">
            <% if (currentPage == 1) { %>
              <li class="page-item disabled"><a class="page-link">First</a></li>
            <% } else { %>
              <li class="page-item"><a href="?page=1&role=<%=userRole%>&filter=<%=filter%>&keys=<%=keys%>" class="page-link">First</a></li>
            <% } %>
            
            <% var i = (Number(currentPage) > 5 ? Number(currentPage) - 4 : 1) %>
                <% if (i !== 1) { %>
                    <li class="page-item disabled"><a class="page-link">...</a></li>
                <% } %>
                <% for (; i <= (Number(currentPage) + 4) && i <= lastPage; i++) { %>
                    <% if (i == currentPage) { %>
                        <li class="page-item active"><a class="page-link"><%= i %></a></li>
                    <% } else { %>
                        <li class="page-item"><a class="page-link" href="?page=<%= i %>&role=<%=userRole%>&filter=<%=filter%>&keys=<%=keys%>"><%= i %></a></li>
                    <% } %>
                    <% if (i == Number(currentPage) + 4 && i < lastPage) { %>
                        <li class="page-item disabled"><a class="page-link">...</a></li>
                    <% } %>
                <% } %>
                <% if (currentPage == lastPage) { %>
                    <li class="page-item disabled"><a class="page-link">Last</a></li>
                <% } else { %>
                    <li class="page-item"><a href="?page=<%= lastPage %>&role=<%=userRole%>&filter=<%=filter%>&keys=<%=keys%>" class="page-link">Last</a></li>
                <% } %>
            </ul>
        </nav>
     <% } %>
            </div>
        </div>
    </div>